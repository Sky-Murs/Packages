{
    "endOfLine": "auto",
    "useTabs": false,
    "printWidth": 100,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "semi": false,
    "arrowParens": "avoid",
    "quoteProps": "as-needed",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrderCaseInsensitive": true
}