{
    "compilerOptions": {
        "lib": ["ES2021", "DOM"],
        "jsx": "react-jsx",
        "module": "ES2022",
        "target": "ES2017",
        "moduleResolution": "node",

        "esModuleInterop": true,

        "typeRoots": [
            "node_modules/@types"
        ],
        "baseUrl": ".",
        "paths": {
            "*": ["includes/*", "node_modules/*"],
            
        }
    },
    "exclude": ["node_modules", "node_modules"]
}
        
